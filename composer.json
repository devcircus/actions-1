{
    "name": "perfect-oblivion/actions",
    "description": "Invokable actions. Not controllers.",
    "version": "0.2.0",
    "keywords": [
        "perfect-oblivion",
        "actions",
        "adr",
        "Laravel"
    ],
    "homepage": "https://github.com/perfect-oblivion/actions",
    "license": "MIT",
    "authors": [
        {
            "name": "Clayton Stone",
            "email": "clay@perfectoblivion.com",
            "homepage": "https://perfectoblivion.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.7.*|5.8.*|~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PerfectOblivion\\Actions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PerfectOblivion\\Actions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PerfectOblivion\\Actions\\ActionServiceProvider"
            ]
        }
    }
}
